{
    // "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
        "skipLibCheck": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "noEmit": true,
        "lib": ["ESNext"],
        "types": ["node"]
    },
    "include": ["vite.config.ts"]
}
