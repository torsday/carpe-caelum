    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "graphql": "^16.6.0",
    "leaflet": "^1.9.4",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@swc/core": "^1.6.13",
    "@swc/helpers": "^0.5.11",
    "@types/leaflet": "^1.7.0",
    "@types/react": "^18.0.9",
