        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-leaflet": "^4.2.1",
        "styled-components": "^6.1.12"
    },
    "devDependencies": {
        "@eslint/create-config": "^1.3.1",
        "@swc/core": "^1.6.13",
